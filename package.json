{
  "name": "innerviso",
  "version": "1.0.0",
  "description": "Software interno da Visonet",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.visonet.innerviso",
    "productName": "InnerViso",
    "directories": {
      "output": "dist"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "BrunoWitt",
        "repo": "InnerViso"
      }
    ],
    "files": [
      "!node_modules/.cache"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "electron-updater": "^6.6.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12"
  },
  "keywords": [],
  "author": "Bruno Witt",
  "license": "ISC"
}
